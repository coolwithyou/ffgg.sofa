    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@neondatabase/serverless": "^1.0.2",
    "@portone/browser-sdk": "^0.1.1",
    "@portone/server-sdk": "^0.18.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "ai": "^6.0.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "inngest": "^3.48.1",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.11.0",
    "nanoid": "^5.1.6",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "openai": "^6.15.0",
    "otplib": "^12.0.1",
    "papaparse": "^5.5.3",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "resend": "^6.6.0",
    "tailwind-merge": "^3.4.0",
    "unpdf": "^1.4.0",
