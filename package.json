{
  "name": "ffgg.sofa",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "dev": "next dev --port 3060",
    "dev:inngest": "npx inngest-cli@latest dev -u http://localhost:3060/api/inngest",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:seed:admin": "dotenv -e .env.local -- tsx scripts/create-admin.ts",
    "db:enable-vector": "dotenv -e .env.local -- tsx scripts/enable-pgvector.ts",
    "db:reprocess": "dotenv -e .env.local -- tsx scripts/reprocess-documents.ts",
    "rag:evaluate": "dotenv -e .env.local -- tsx scripts/evaluate-rag.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
